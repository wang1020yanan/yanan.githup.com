body,html{
    margin: 0;
    padding: 0;
    font-family:"Microsoft YaHei","华文细黑","Helvetica","Comic Sans MS","黑体",sans-serif;

}
body{
    display: none;
    background: #f4f4f4;
}
ul,ol{
    margin: 0;
}
*{
    margin: 0;
    padding: 0;
}
a{
    outline: none;
    text-decoration: none!important;
    border: none 0;
}
a img{
    border: 0 none;
}
li{
    list-style: none;
}

/*组件*/
.c1{
    width: 8.33333%;
}
.c2{
    width:16.666666%;
}
.c3{
    width: 25%;
}
.c4{
    width: 33.333%;
}
.c9{
    width: 75%;
}
.c7{
    width: 58.333333%;
}
.c6{
    width: 50%;
}
.c12{
    width: 100%;
}
.c2{
    width: 16.666%;
}
.c8{
    width: 66.666%;
}
.c5{
    width: 41.666%;
}
.c11{
    width: 0.91666%;
}
.c10{
    width: 83.33333%;
}
.c3,.c9 ,.c12 ,.c6,.c2,.c4,.c5,.c11,.c1,.c7,.c8,.c10{
    position: relative;
    float: left;
}
.container{
    padding: 0;
    width: 970px;
    margin: 0 auto;
}
.full{
    width: 100%;
    min-width: 970px;
    padding: 0;
    margin: 0;
}




/*胶原信息*/
.teacher_wrap{
    padding: 25px 0 12px 0;
    margin-top: 20px;
    border: 1px solid #cccccc;
    box-shadow: 1px 1px 5px #cccccc;
    background: #FFFFFF;
}
.tc_img{
    text-align: center;
    display: block;
}
.tc_img img{
    width: 120px;
    height: 120px;
    vertical-align:middle;
}
.tc_name{
    /*position: absolute;*/
    width: 120px;
    height: 30px;
    color: #FFFFFF;
    line-height: 30px;
    font-size: 15px;
}
.tc_name p i{
    font-size: 14px;
}
.tc_pj{
    line-height: 23px;
}
.tc_sc{
    font-size: 18px;
    /*font-weight: 600;*/
}
.tc_tip span:nth-child(1){
    color: #FFFFFF;
    background: #F6970E;
    padding: 1px 3px;
    -webkit-border-radius:3px;
    -moz-border-radius: 3px;
    border-radius: 3px;
    font-size: 14px;
}
.tc_tip span:nth-child(2){
    color: #FFFFFF;
    background:#0a74f6;
    padding: 1px 3px;
    -webkit-border-radius:3px;
    -moz-border-radius: 3px;
    border-radius: 3px;
    font-size: 14px;
}
.tc_lev{
    color: #F6970E;
}
.tc_price{
    text-align: center;
    right: 30px;
}
.tc_price p:nth-child(1){
    font-size: 28px;
    /*font-weight: 600;*/
    color: #F6970E;
}
.tc_choice a{
    -webkit-border-radius:5px;
    -moz-border-radius: 5px;
    border-radius: 5px;
    font-size: 16px;
    padding: 8px 30px;
    color: #FFFFFF;
    -webkit-transition: color,background .3s;
    -moz-transition: color,background .3s;
    -ms-transition:color,background .3s;
    -o-transition: color,background .3s;
    transition: color,background .3s;
}
.tc_choice{
   padding-top: 32px;
    /*margin-top: 10px;*/
}
.tc_choice a{
    background: #509EFD;
}
.tc_choice a:hover{
    background: #0a74f6;
}
.tc_focus{
    padding-top: 20px;
}
.tc_focus a{
    -webkit-border-radius: 4px;
    -moz-border-radius: 4px;
    border-radius: 4px;
    padding: 5px 10px;
    margin: 0 5px;
    color: #A1A1A1;
    -webkit-transition: color,background .4s;
    -moz-transition:  color,background  .4s;
    -ms-transition:  color,background  .4s;
    -o-transition:  color,background  .4s;
    transition:  color,background  .4s;
}
.tc_focus a:hover{
    background:#FF2492;
    color: #FFFFFF;
}

.teacher_wrap:hover{
    background: #EEFBFD;
}
.tc_slg{
    color: #716F6F;
    font-size: 16px;
}
.tc_address{
    color: #A1A1A1;
    font-size: 15px;
}
.tc_address img{
    vertical-align: middle;
}
/*教员课程*/
.detail_wrap{
    margin-top: 20px;
}
.tc_course{
    border: 1px solid #CCCCCC;
    padding: 20px;
    background: #FFFFFF;
}
.tc_course .tit span{
    display: inline-block;
    padding-right: 20px;
    font-size: 18px;
    border-bottom: 4px solid #0a74f6;
}
.course_img{
    padding: 5px 10px;
}
.course_wrap{
    padding: 20px 0;
}
.course_wrap img{
    width: 100%;
}
/*授课时段*/
.course_time{
    margin: 20px 0;
    text-align: center;
}
.course_time li{
    display: inline-block;
    width: 38px;
    padding: 7px 0;
    font-size: 16px;
}
/*详细信息*/
.tc_detail{
    border: 1px solid #CCCCCC;
    width: 545px;
    margin-left: 20px;
    padding: 20px;
    background: #FFFFFF;
}
.back{
    font-size: 18px;
    margin-bottom: 35px;
}
.back p{
    color: #AFADAD;
    font-size: 16px;
    padding-top: 3px;
}
.back span:nth-child(1){
   border-left: 6px solid #0a74f6;
    display: inline-block;
    height: 18px;
    position: relative;
    top: 3px;
}
.comments{
    margin-bottom: 10px;
}
.comments_tip{
    padding: 5px 15px
    /*text-align: center;*/
}
.comments_tip span{
    background: #FF9425;
    display: inline-block;
    padding: 5px 10px;
    color: #FFFFFF;
    -webkit-border-radius: 15px;
    -moz-border-radius: 15px;
    border-radius: 15px;
}
.comments_tip span:before{
    /*content: "";*/
    border-style: solid;
    border-width: 17px 15px 14px 0;
    width: 0;
    height: 0;
    position: absolute;
    border-color: rgb(255, 255, 255) #FF9425 rgb(255, 255, 255) rgb(255, 255, 255);
    top: 5px;
    left: 6px;
}
.comments_model{
    margin: 15px 0;
}
.comments_lev{
    margin: 10px 0 20px;
    font-size: 16px;
}
.comments_lev span{
    color: #FF9425;
    display: inline-block;
    padding-left: 10px;
}
.comments_name{
    font-size: 18px;
}
.comments_time{
    text-align: right;
    color: #A1A1A1;
}
.comments_content{
    font-size: 15px;
    color:#868686;
}
.comments_all{
    text-align: right;
    text-decoration: underline;
}
/*底部*/
.footer{
    height: 82px;
    background: #151818;
    color: #fafafa;
}
.foo_logo{
    line-height: 20px;
}
.foo_logo img{
    padding-top: 17px;
}
.co_name{
    padding-top: 18px;
}
.foo_wx{
    text-align: center;
}
.foo_wx  img{
    padding-top: 12px;
}
.foo_wx p{
    font-size: 18px;
    padding-top: 17px;
}
.foo_call .c1{
    text-align:right;
}
.foo_call img{
    padding-top: 17px;
    /*top: 3px;*/
    position: relative;
}
.foo_call .call{
    width: 360px;
    margin-left: 30px;
    position: relative;
    padding-top: 12px;
}
.foo_call .c11{
    margin-top: 6px;
}
.foo_call .call img{
    padding-top: 0;
}
.foo_call .add{
    top: -6px;
}
.foo_call p{
    display: inline-block;
    line-height: 4px;
}