body,html{
    margin: 0;
    padding: 0;
    font-family:"Microsoft YaHei","幼圆","华文细黑","Helvetica","Comic Sans MS","黑体",sans-serif;

}
body{
    display: none;
    background: #f4f4f4;
}
*{
    margin: 0;
    padding: 0;
}
a{
    outline: none;
    text-decoration: none;
    border: none 0;
}
a img{
    border: 0 none;
}
li{
    list-style: none;
}

