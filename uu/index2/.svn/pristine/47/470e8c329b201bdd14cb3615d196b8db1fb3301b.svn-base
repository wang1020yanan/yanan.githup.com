.container{
    width: 970px;
    margin: 0 auto;
}
.full{
    width: 100%;
    min-width: 970px;
    padding: 0;
    margin: 0;
}
/*订单*/
.p_order_wrap{
min-height: 900px;
}
.p_order{
    background: #FFFFFF;
    -webkit-transition: border .3s;
    -moz-transition: border .3s;
    -ms-transition:  border .3s;
    -o-transition:  border .3s;
    transition:  border .3s;
    border-top: 2px solid  #F4F4F4;
    border-bottom: 1px  solid #F4F4F4;
    border-right: 1px  solid #F4F4F4;
    border-left: 1px solid #F4F4F4;
    margin-top: 30px;
}
.p_order:hover{
    border-top: 2px solid  #54C5F5;
    border-bottom: 1px  solid #54C5F5;
    border-right: 1px  solid #54C5F5;
    border-left: 1px solid #54C5F5;
}
.order_a{
    /*background-image: url("../img/28.png");*/
    background-size: 100% 100%;
}
.order_b ul{
    padding: 18px 30px;
}

.order_b ul li{
    line-height: 40px;
    font-size: 14px;
    color: #777777;
}
.order_b ul i{
    color:#54C5F5;
    font-size: 10px;
}
.order_c ul li{
    line-height: 30px;
}
.order_c ul{
    padding: 46px 0;
}
.order_c ul li:nth-child(1){
    font-size: 28px;
    color: #F6970E;
    text-align: center;
}
.order_c ul li:nth-child(1) span{
    font-size: 18px;
    color:#777777;
}
.order_c ul li:nth-child(2){
    font-size: 16px;
    color:#777777;
}
.order_e{
    text-align: center;
}
.order_e img{
    width: 70px;
    height: 70px;
    -webkit-border-radius:50%;
    -moz-border-radius: 50%;
    border-radius: 50%;
    margin-top: 43px;
}
.order_f{
    line-height: 30px;
    margin-top: 50px;
}
.order_f p:nth-child(1){
    color: #777777;
}
.order_f p:nth-child(2){
    color: #F6970E;
}
.order_g{
     background-image: url("../img/27.png");
    background-size: 100% 100%;
    line-height: 26px;
    color: #FFFFFF;
    margin-top: 8px;
    font-size: 18px;
}
.order_h img{
    margin-top: 15px;
    width: 101px;
}
.sanjiao{
    border-color: #fff #F4F4F4 #fff #fff;
    border-style: solid;
    border-width: 18px 18px 0 0;
    width: 0;
    height: 0;
    position: relative;
    left: 466px;
    top: 137px;
}



/*底部*/
.footer{
    height: 82px;
    background: #151818;
    color: #fafafa;
}
.foo_logo{
    line-height: 20px;
}
.foo_logo img{
    padding-top: 14px;
}
.co_name{
    padding-top: 21px;
}
.foo_wx{
    text-align: center;
}
.foo_wx  img{
    padding-top: 11px;
}
.foo_wx p{
    font-size: 18px;
    padding-top: 20px;
}
.foo_call .c1{
    text-align:right;
}
.foo_call img{
    padding-top: 13px;
    top: 3px;
    position: relative;
}
.foo_call .call{
    width: 360px;
    margin-left: 30px;
    position: relative;
    top: 4px;
}
.foo_call .add{
    top: -6px;
}
.foo_call p{
    display: inline-block;
}