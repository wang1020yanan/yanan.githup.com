body,html{
    margin: 0;
    padding: 0;
    font-family:"幼圆","华文细黑","Helvetica","Comic Sans MS","黑体",sans-serif;
}
body{
    display: none;
}
*{
    margin: 0;
    padding: 0;
}
a{
    outline: none;
    text-decoration: none;
    border: none 0;
}
a img{
    border: 0 none;
}
li{
    list-style: none;
}

/*组件*/
.c1{
    width: 8.33333%;
}
.c2{
    width:16.666666%;
}
.c3{
    width: 25%;
}
.c4{
    width: 33.333%;
}
.c9{
    width: 75%;
}
.c7{
    width: 58.333333%;
}
.c6{
    width: 50%;
}
.c12{
    width: 100%;
}
.c2{
    width: 16.666%;
}
.c8{
    width: 66.666%;
}
.c5{
    width: 41.666%;
}
.c11{
    width: 91.666%;
}
.c10{
    width: 83.33333%;
}
.c3,.c9 ,.c12 ,.c6,.c2,.c4,.c5,.c11,.c1,.c7,.c8,.c10{
    position: relative;
    float: left;
}
.container{
    width: 970px;
    margin: 0 auto;
}
.full{
    width: 100%;
    min-width: 970px;
    padding: 0;
    margin: 0;
}
/*头部*/
.header{
    width: 100%;
    height: 100px;
    min-width: 970px;
    overflow: hidden;
    background-image: url("../img/center/1.png");
    background-size: 100% 100%;
}
.header img:hover{
    opacity: 0.8;
}
.header .c7 img{
    vertical-align: middle;
    margin-top: 21px;
}
.header .c5{
    text-align: right;
}
.header .c5 p{
    line-height: 100px;
    font-weight: 600;
}
.header .c5 .c3{
    top: 38px;
}
.header .c5 .c3 img{
    cursor: pointer;
}
   /*内容*/

/*菜单*/
.menu{
    width: 226px;
    background: #EAEAEA;
    height: 300px;
}
.menu .c12{
    border-left: 3px solid #EAEAEA;
    height: 60px;
    margin-top: 30px;
}
.menu .c12 .c4{
    text-align: right;
    margin-top: 17px;
}
.menu .c12 .c8 p{
    margin-top: 17px;
    padding-left: 10px;
    font-size: 20px;
    cursor: pointer;
    font-weight: 700;
    color: #5c5c5c;
}
.menu .c12 .c8 p:hover{
    color: #0EB621;
}
.menu .wrap1 .c12:nth-child(1) {
    border-left: 3px solid #0EB621;
}
.menu .wrap1 div:nth-child(1) .c8 p{
    color: #0EB621;
}
/*进度条*/
.progress{
    height: 150px;
    background: #EAEAEA;
    margin-bottom: 10px;
}
.progress img, .progress p{
    position: relative;
    font-size: 18px;
    font-weight: 600;
}
.progress div:nth-child(1) img{
    left: 115px;
    top: 30px;
}
.progress div:nth-child(1) p{
    left: 115px;
    top: 31px;
    color: #0EB621;
}
.progress div:nth-child(2) img{
    top: 38px;
}
.progress div:nth-child(2) p{
    left: 127px;
    top: 46px;
    color: #5c5c5c;
}
.progress div:nth-child(3) img{
    top: 38px;
}
.progress div:nth-child(3) p{
    left: 127px;
    top: 46px;
    color: #5c5c5c;
}
/*菜单内容*/
.con{
    left: 50px;
    width: 727px;
    background: #EAEAEA;
    margin-bottom: 50px;
    padding-bottom: 50px;
}
.con .c10{
    margin-left: 61px;
}
.pic{
    text-align: center;
    margin: 70px 0;
}
.pic img{
    cursor: pointer;
    border-radius: 100px;
    border: 2px solid #0EB621;
}
.sign{
    cursor: pointer;
    position: absolute;
    left: 252px;
    top: 3px;
    opacity: 0;
    height: 100px;
    width: 100px;
}
/*.pic img:nth-child(2){*/
    /*cursor: pointer;*/
    /*position: absolute;*/
    /*left: 285px;*/
    /*top: 40px;*/
/*}*/
.con .c10 p{
    color: #5c5c5c;
    font-size: 24px;
    font-weight: 600;
}
.basic{
    margin-top: 30px;
}
.basic .c9 p{
    color: #000;
}
.input{
    margin-top: 30px;
}
.input .c9 input {
     height: 28px;
     outline-color: #0EB621;
     width: 99%;
}
.input .c9 select{
    height: 32px;
    outline-color: #0EB621;
    width: 454px;
}
.new{
    margin-top: 30px;
}
.new textarea{
    width: 453px;
    height: 150px;
    outline-color: #0EB621;
}
.warning{
    text-align: right;
    margin-top: 20px;
}
.warning .c12 p{
    font-size: 18px;
    color: red;
}
.con_ok{
    text-align: right;
    margin-top: 30px;
}
.con_ok img{
    cursor: pointer;
}
.con_ok img:hover{
    opacity: 0.8;
}
    /*底部*/
.footer{
    height: 82px;
    background: #151818;
    color: #fafafa;
}
.foo_logo{
    line-height: 20px;
}
.foo_logo img{
    padding-top: 14px;
}
.co_name{
    padding-top: 21px;
}
.foo_wx{
    text-align: center;
}
.foo_wx  img{
    padding-top: 11px;
}
.foo_wx p{
    font-size: 18px;
    padding-top: 20px;
}
.foo_call .c1{
    text-align:right;
}
.foo_call img{
    padding-top: 13px;
    top: 3px;
    position: relative;
}
.foo_call .call{
    width: 360px;
    margin-left: 30px;
    position: relative;
    top: 4px;
}
.foo_call .add{
    top: -6px;
}
.foo_call p{
    display: inline-block;
}



