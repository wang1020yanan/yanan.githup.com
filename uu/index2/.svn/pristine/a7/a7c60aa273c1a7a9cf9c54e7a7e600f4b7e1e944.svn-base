body,html{
    margin: 0;
    padding: 0;
    font-family:"微软雅黑","华文细黑","Helvetica","Comic Sans MS","黑体",sans-serif;

}
body{
    display: none;
    background: #f4f4f4;
}
ul,ol{
    margin: 0;
}
*{
    margin: 0;
    padding: 0;
}
a{
    outline: none;
    text-decoration: none!important;
    border: none 0;
}
a img{
    border: 0 none;
}
li{
    list-style: none;
}

/*组件*/
.c1{
    width: 8.33333%;
}
.c2{
    width:16.666666%;
}
.c3{
    width: 25%;
}
.c4{
    width: 33.333%;
}
.c9{
    width: 75%;
}
.c7{
    width: 58.333333%;
}
.c6{
    width: 50%;
}
.c12{
    width: 100%;
}
.c2{
    width: 16.666%;
}
.c8{
    width: 66.666%;
}
.c5{
    width: 41.666%;
}
.c11{
    width: 0.91666%;
}
.c10{
    width: 83.33333%;
}
.c3,.c9 ,.c12 ,.c6,.c2,.c4,.c5,.c11,.c1,.c7,.c8,.c10{
    position: relative;
    float: left;
}
.container{
    padding: 0;
    width: 970px;
    margin: 0 auto;
}
.full{
    width: 100%;
    min-width: 970px;
    padding: 0;
    margin: 0;
}
.hid{
    display: none;
}




/*教员概述*/
.teacher_wrap{
    /*padding: 8px 0 23px 20px;*/
    margin-top: 20px;
    /*border: 1px solid #cccccc;*/
    /*box-shadow: 1px 1px 5px #cccccc;*/
}
.choose_tit{
    font-size: 18px;
}
.choose_tit i{
    padding-top: 15px;
    color: #2e8ded;
}
.choose_tit span{
    padding-top: 15px;
}
.tc_img{
    text-align: center;
    display: block;
}
.tc_img img{
    width: 80px;
    height: 80px;
    vertical-align:middle;
}
.tc_name{
    /*position: absolute;*/
    width: 120px;
    height: 30px;
    color: #FFFFFF;
    line-height: 30px;
    font-size: 15px;
}
.tc_name p i{
    font-size: 14px;
}
.tc_pj{
    line-height: 23px;
    /*padding: 15px 0 0 10px;*/
}
.tc_slg{
    font-size: 16px;
}
.tc_sc{
    font-size: 16px;
    color: #868686;
    /*font-weight: 600;*/
}
.tc_sc i{
    color: #0880d7;
}
/*订单进度*/
.pro{
    margin-top: 30px;
}
.pro_wrap{
    padding: 20px 57px;
    border: 1px solid #cccccc;
    background: #FFFFFF;
}
.pro_tit{
    text-align: center;
    color: #0880d7;
    font-size: 18px;
}
.pro-o{
    overflow: hidden;
}
.pro-o{
    font-size: 20px;
    color: #CECBCB;
}
.pro-o i{
    font-size: 25px;
}
.pro-o span{
    width: 385px;
    display:inline-block;
    font-weight: 900;
    border-bottom: 2px solid #CECBCB;
    border-top: 2px solid #CECBCB;
    line-height: 0px;
    position: relative;
    left: -8px;
    top: -7px;
}
.pro-wrap div:nth-child(1){
    left: 32px;
    color: #0880d7;
}
.pro-wrap div:nth-child(1) span{
    border-color: #0880d7;
}
.pro-wrap div:nth-child(2) span{
    border-color: #0880d7;
}
.pro-wrap div:nth-child(3){
  right: 35px;
}
.pro-wrap div:nth-child(3) span{
}
.pro_ab {
    text-align: center;
    font-size: 16px;
    color: #CECBCB;
}
.pro_ab div:nth-child(1){
    left: -100px;
}
.pro_ab div:nth-child(2){
    left: -60px;
}
.pro_ab div:nth-child(3){
    left: 53px;
}
.pro_ab div:nth-child(4){
    left: 16px;
}
/*抢单教员*/
.need_tea{
    margin-top: 30px;
    padding: 30px;
    background: #FFFFFF;
    border: 1px solid #CCCCCC;
}
.tea_tit{
    font-size: 18px;
}
.tea_tit:before{
    display: inline-block;
     content: "..";
    background: #0880d7;
    color: #0880d7;
    height: 20px;
    position: relative;
    top: 3px;
}
.sort_span{
    font-size: 13px;
    display: inline-block;
    color: #FFFFFF;
    background: #2e8ded;
    width: 17px;
    text-align: center;
    height: 17px;
    -webkit-border-radius: 100%;
    -moz-border-radius: 100%;
    border-radius: 100%;
    position: relative;
    bottom: 1px;
}
.tc_a{
    font-size: 20px;
}
.need_demo{
    padding: 10px 30px;
}

/*胶原信息*/
.teacher_wrap{
    /*padding: 20px 0 10px 0;*/
    margin-top: 10px;
    /*border: 1px solid #cccccc;*/
    /*box-shadow: 1px 1px 5px #cccccc;*/
}
.tc_img{
    text-align: center;
    display: block;
    top: 2px;
}
.tc_img img{
    width: 50px;
    height: 50px;
    vertical-align:middle;
}
.tc_name{
    position: absolute;
    background: rgba(0,0,0,0.7);
    width: 120px;
    height: 30px;
    color: #FFFFFF;
    left: 30px;
    bottom: 0;
    line-height: 30px;
    font-size: 15px;
}
.tc_name p i{
    font-size: 14px;
}
.tc_pj{
    line-height: 23px;
}
.tc_sc{
    font-size: 18px;
    /*font-weight: 600;*/
}
.tc_tip span:nth-child(1){
    color: #FFFFFF;
    background: #F6970E;
    padding: 1px 3px;
    -webkit-border-radius:3px;
    -moz-border-radius: 3px;
    border-radius: 3px;
    font-size: 14px;
}
.tc_tip span:nth-child(2){
    color: #FFFFFF;
    background:#0a74f6;
    padding: 1px 3px;
    -webkit-border-radius:3px;
    -moz-border-radius: 3px;
    border-radius: 3px;
    font-size: 14px;
}
.tc_lev{
    color: #F6970E;
}
.tc_price{
    text-align: right;
    right: 30px;
    line-height: 40px;
}

.tc_price p:nth-child(1){

    font-size: 28px;
    /*font-weight: 600;*/
    color: #F6970E;
}
.tc_price p:nth-child(2){
    margin: 0;
    color: #A1A1A1;
    font-size: 16px;
    position: relative;
    top: -20px;

    /*font-weight: 600;*/
}
.tc_choice a{
    -webkit-border-radius: 3px;
    -moz-border-radius: 3px;
    border-radius: 3px;
    font-size: 20px;
    padding: 6px 37px;
    color: #FFFFFF;
    -webkit-transition: color,background .3s;
    -moz-transition: color,background .3s;
    -ms-transition: color,background .3s;
    -o-transition: color,background .3s;
    transition: color,background .3s;
}
.tc_choice{
    line-height: 30px;
    margin-top: 8px;
}
.tc_choice a:nth-child(1){
    background: #4C9AF9;
}
.tc_choice a:nth-child(2){
    background:#EFB462;
}
.tc_choice a:nth-child(1):hover{
    background: #0a74f6;
}
.tc_choice a:nth-child(2):hover{
    background: #F6970E;
}
/*.teacher_wrap:hover{*/
    /*background:#EEFBFD;*/
/*}*/
.tc_slg{
    color: #A1A1A1;
}
.more{
    position: absolute;
    left: 395px;
    top: 70px;
    background:#CCCCCC;
    padding: 2px 3px;
    cursor: pointer;
    z-index: 9999;
}
.less{
    position: absolute;
    left: 395px;
    bottom: 0;
    background: #CCCCCC;
    padding: 3px 6px;
    cursor: pointer;
    z-index: 9999;
}
.more:hover{
    background: #EAE4E4;
}
.less:hover{
    background: #EAE4E4;
}
.line{
    border-bottom: 6px solid #CCCCCC;
}
/*详细信息*/
.tc_detail{
    /*border: 1px solid #CCCCCC;*/
    /*width: 545px;*/
    /*margin-left: 20px;*/
    padding: 20px;
    background:#F1F1F1 ;
    display: none;
}
.back{
    font-size: 18px;
    margin-bottom: 35px;
}
.back p{
    color: #AFADAD;
    font-size: 16px;
    padding-top: 3px;
}
.tc_detail .back span:nth-child(1){
    border-left: 6px solid #0a74f6;
    display: inline-block;
    height: 18px;
    position: relative;
    top: 3px;
}
.comments{
    margin-bottom: 10px;
}
.comments_tip{
    padding: 5px 15px
    /*text-align: center;*/
}
.comments_tip span{
    background: #FF9425;
    display: inline-block;
    padding: 5px 10px;
    color: #FFFFFF;
}
.comments_tip span:before{
    content: "";
    border-style: solid;
    border-width: 17px 15px 14px 0;
    width: 0;
    height: 0;
    position: absolute;
    border-color: #F1F1F1 #FF9425 #F1F1F1 rgb(255, 255, 255);
    top: 5px;
    left: 6px;
}
.comments_model{
    margin: 15px 0;
}
.comments_lev{
    margin: 10px 0 20px;
    font-size: 16px;
}
.comments_lev span{
    color: #FF9425;
    display: inline-block;
    padding-left: 10px;
}
.comments_name{
    font-size: 18px;
}
.comments_time{
    text-align: right;
    color: #A1A1A1;
}
.comments_content{
    font-size: 15px;
    color:#868686;
}
.comments_all{
    text-align: right;
    text-decoration: underline;
}


/*订单信息*/
.order{
    margin-top: 30px;
}
.order_wrap{
    padding: 20px;
    border: 1px solid #cccccc;
    background: #FFFFFF;
}
.back{
    font-size: 18px;
    margin-bottom: 35px;
}
.back p{
    color:#8C8A8A;
    font-size: 16px;
    padding-top: 3px;
}
.ul_tit{
    border-left: 6px solid #0a74f6;
    display: inline-block;
    height: 18px;
    position: relative;
    top: 3px;
}
.order_detail{
    font-size: 14px;
    padding: 20px;
}
.price{
    font-size: 25px;
    color: #F54464;
}
.pay img{
    width: 25px;
}
.pay span{
    -webkit-border-radius: 3px;
    -moz-border-radius: 3px;
    border-radius: 3px;cursor: pointer;
    font-size: 15px;
    padding: 8px;
    display: inline-block;
    border: 1px solid #CCCCCC;
    width: 125px;
}
.pay span i{
    position: absolute;
    color: #0880d7;
    margin-top: 18px;
    margin-left: -6px;
}
.pay span:nth-child(2) i{
    position: absolute;
    color: #0880d7;
    margin-top: 18px;
    margin-left: 9px;
}
.pay_btn{
    text-align: center;
    margin-bottom: 50px;
}
/*底部*/
.footer{
    height: 82px;
    background: #151818;
    color: #fafafa;
}
.foo_logo{
    line-height: 20px;
}
.foo_logo img{
    padding-top: 17px;
}
.co_name{
    padding-top: 18px;
}
.foo_wx{
    text-align: center;
}
.foo_wx  img{
    padding-top: 12px;
}
.foo_wx p{
    font-size: 18px;
    padding-top: 17px;
}
.foo_call .c1{
    text-align:right;
}
.foo_call img{
    padding-top: 17px;
    /*top: 3px;*/
    position: relative;
}
.foo_call .call{
    width: 360px;
    margin-left: 30px;
    position: relative;
    padding-top: 12px;
}
.foo_call .c11{
    margin-top: 6px;
}
.foo_call .call img{
    padding-top: 0;
}
.foo_call .add{
    top: -6px;
}
.foo_call p{
    display: inline-block;
    line-height: 4px;
}