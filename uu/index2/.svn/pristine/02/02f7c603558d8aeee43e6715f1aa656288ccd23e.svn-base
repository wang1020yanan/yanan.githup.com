/*弹窗*/
.fix_window{
    position: fixed;
    width: 100%;
    height: 100%;
    top: 0;
    background:rgba(0,0,0,0.9);
    z-index: 1000;
    opacity: 0.9;
    display: none;
}
/*找老师窗口*/
.fix_wrap_find{
    display: none;
    z-index: 1000;
    width: 340px;
    height: 330px;
    position: fixed;
    border: 2px solid #5FBBFF;
    background: #FFFFFF;
    top: 50%;
    left: 50%;
    margin-left: -170px;
    margin-top: -165px;
}
.fix_wrap_find p{
    margin-bottom: 2px;
    color: #FFFFFF;
}
.fix_wrap_find .findtit{
    text-align: center;
    font-size: 30px;
    line-height: 55px;
    background:  #5FBBFF;
}
.fix_wrap_find .find_close{
    color: #FFFFFF;
    position: absolute;
    left: 300px;
}
.fix_wrap_find .find_close:hover{
    color: #CCCCCC;
    cursor: pointer;
}
.fix_wrap_find .findabout{
    padding: 35px 60px;
}
.fix_wrap_find .findabout p{
    color: #5FBBFF;
    font-size: 15px;
}
.fix_wrap_find .findbutton{
    text-align: center;
    padding-top: 30px;
}
.fix_wrap_find .findbutton a{
    height: 40px;
    line-height: 40px;
    width: 200px;
}
