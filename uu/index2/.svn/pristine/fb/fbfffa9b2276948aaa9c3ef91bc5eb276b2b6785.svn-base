body,html{
    margin: 0;
    padding: 0;
    font-family:"微软雅黑","华文细黑","Helvetica","Comic Sans MS","黑体",sans-serif;

}
body{
    display: none;
    background: #f4f4f4;
}
ul,ol{
    margin: 0;
}
*{
    margin: 0;
    padding: 0;
}
a{
    outline: none;
    text-decoration: none!important;
    border: none 0;
}
a img{
    border: 0 none;
}
li{
    list-style: none;
}

/*组件*/
.c1{
    width: 8.33333%;
}
.c2{
    width:16.666666%;
}
.c3{
    width: 25%;
}
.c4{
    width: 33.333%;
}
.c9{
    width: 75%;
}
.c7{
    width: 58.333333%;
}
.c6{
    width: 50%;
}
.c12{
    width: 100%;
}
.c2{
    width: 16.666%;
}
.c8{
    width: 66.666%;
}
.c5{
    width: 41.666%;
}
.c11{
    width: 0.91666%;
}
.c10{
    width: 83.33333%;
}
.c3,.c9 ,.c12 ,.c6,.c2,.c4,.c5,.c11,.c1,.c7,.c8,.c10{
    position: relative;
    float: left;
}
.container{
    padding: 0;
    width: 970px;
    margin: 0 auto;
}
.full{
    width: 100%;
    min-width: 970px;
    padding: 0;
    margin: 0;
}
.hid{
    display: none;
}

/*支付成功*/
.pro{
    margin-top: 30px;
}
.pro_wrap{
    padding: 150px 280px;
    border: 1px solid #cccccc;
    text-align: left;
    background: #FFFFFF;
}
.pay_ok i{
    color: #0880d7;
    font-size: 120px;
}
.price_wrap{
    padding: 60px 0 60px 0;
    font-size: 18px;
}
.price_wrap p span{
    color: #0880d7;
}
.price_wrap p a{
    text-decoration:underline!important;
}
.pay_time{
    text-align: center;
}
.pay_time span{
    color: #0880d7;
}
/*底部*/
.footer{
    height: 82px;
    background: #151818;
    color: #fafafa;
}
.foo_logo{
    line-height: 20px;
}
.foo_logo img{
    padding-top: 17px;
}
.co_name{
    padding-top: 18px;
}
.foo_wx{
    text-align: center;
}
.foo_wx  img{
    padding-top: 12px;
}
.foo_wx p{
    font-size: 18px;
    padding-top: 17px;
}
.foo_call .c1{
    text-align:right;
}
.foo_call img{
    padding-top: 17px;
    /*top: 3px;*/
    position: relative;
}
.foo_call .call{
    width: 360px;
    margin-left: 30px;
    position: relative;
    padding-top: 12px;
}
.foo_call .c11{
    margin-top: 6px;
}
.foo_call .call img{
    padding-top: 0;
}
.foo_call .add{
    top: -6px;
}
.foo_call p{
    display: inline-block;
    line-height: 4px;
}