body,html{
    margin: 0;
    padding: 0;
    height: 100%;
}
body{
    display: none;
}
*{
    font-family:"幼圆","Helvetica","Comic Sans MS","黑体",sans-serif;
    margin: 0;
    padding: 0;
}
a{
    outline: none;
    text-decoration: none;
    border: none 0;
}
a img{
    border: 0 none;
}
li{
    list-style: none;
}
input:-webkit-autofill {
    -webkit-box-shadow: 0 0 0px 1000px white inset;
}

/*组件*/
.c1{
    width: 8.33333%;
}
.c2{
    width:16.666666%;
}
.c3{
    width: 25%;
}
.c4{
    width: 33.333%;
}
.c9{
    width: 75%;
}
.c7{
    width: 58.333333%;
}
.c6{
    width: 50%;
}
.c12{
    width: 100%;
}
.c2{
    width: 16.666%;
}
.c8{
    width: 66.666%;
}
.c5{
    width: 41.666%;
}
.c11{
    width: 0.91666%;
}
.c10{
    width: 83.33333%;
}
.c3,.c9 ,.c12 ,.c6,.c2,.c4,.c5,.c11,.c1,.c7,.c8,.c10{
    position: relative;
    float: left;
}
.container{
    width: 970px;
    margin: 0 auto;
}
.full{
    width: 100%;
    min-width: 970px;
    padding: 0;
    margin: 0;
}
.hid{
    display: none;
}
.tip_box{
    position: fixed;
    top: 50%;
    left: 50%;
    width: 200px;
    height: 50px;
    margin-left: -100px;
    margin-top: -50px;
    background: rgba(0,0,0,0.8);
    text-align: center;
    line-height: 50px;
    font-size: 20px;
    color: #ffffff;
    z-index: 100000;
    border-radius: 5px;
    display: none;
}
/*内容*/
.body_con{
    height:130%;
    max-height: 1300px;
    background-image: url("../img/login/1.png");
    overflow: hidden;
    background-position: center top;
    background-size: 100% 112%;
}
.login_head{
    padding: 60px 0;
}
.login_head .c6 img:nth-child(2){
    position: relative;
    top: -14px;
}
.head_stu{
    text-align: right;
}
.head_stu a{
    display: inline-block;
    padding-top: 15px;
}
.login_wrap .c8{
    width: 600px;
    height: 350px;
}
.login_wrap .c4{
    overflow: hidden;
    width: 370px;
    height: 347px;
    background: #fff;
    border-bottom: 1px solid #c5c5c5;
    border-top: 2px solid #0EB621;
}
.login_title{
    left: 60px;
    text-align: center;
    margin-top: 30px;
    font-size: 18px;
}
.login_title .c6{
    width: 125px;
    cursor: pointer;
    padding-bottom: 10px;
    border-bottom: 2px solid #c5c5c5;
}
.login_title div:nth-child(1){
    border-bottom: 2px solid  #0EB621;
    color: #0EB621;
}
.con_wrap{
    width: 250px;
    height: 222px;
    position: relative;
    margin-top: 91px;
    left: 60px;
}
.con_wrap .input img{
    position: absolute;
}
.con_wrap .input{
    width: 248px;
    height: 35px;
    border: 1px solid #0EB621;

}
.con_wrap .input input{
    width: 200px;
    height: 35px;
    outline: none;
    border: none;
    margin-left: 30px;
}
.con_wrap div:nth-child(1) img{
    left: 13px;
    top: 9px;
}
.con_wrap div:nth-child(1){
    margin-bottom: 20px;
}
.con_wrap div:nth-child(2) img{
    left: 13px;
    top: 9px;
}
.forget{
    margin-top: 3px;
}
.forget p{
    cursor: pointer;
    padding-top: 12px;
    font-size: 14px;
}
.log_btn{
    margin-top: 15px;
}
.tc_er{
    left: 100px;
    top: 26px;
}
.tc_er_title{
    font-weight: 600;
    font-size: 18px;
    color: #0EB621;
    left: 102px;
    top: 51px;
}
.con_wrap img:hover{
    opacity: 0.9;
}
/*special*/
.special{
    top: 40px;
    text-align: center;
}
.special .c3 .c8{
    top: 8px;
}
.spe_bg{
    background:rgba(255,255,255,0.6);
    height: 230px;
}
.special p{
    text-align: center;
    margin-top: 5px;
}

/*底部*/
.footer{
    height: 82px;
    background: #151818;
    color: #fafafa;
}
.foo_logo{
    line-height: 20px;
}
.foo_logo img{
    padding-top: 14px;
}
.co_name{
    padding-top: 21px;
}
.foo_wx{
    text-align: center;
}
.foo_wx  img{
    padding-top: 11px;
}
.foo_wx p{
    font-size: 18px;
    padding-top: 20px;
}
.foo_call .c1{
    text-align:right;
}
.foo_call img{
    padding-top: 13px;
    top: 3px;
    position: relative;
}
.foo_call .call{
    width: 360px;
    margin-left: 30px;
    position: relative;
    top: 4px;
}
.foo_call .add{
    top: -6px;
}
.foo_call p{
    display: inline-block;
}
/*弹窗*/
.fix_window{
    position: fixed;
    width: 100%;
    height: 100%;
    top: 0;
    background:rgba(0,0,0,0.9);
    z-index: 1000;
    opacity: 0.9;
    display: none;
}
.fix_wrap{
    font-family:"幼圆","华文细黑","Helvetica","Comic Sans MS","黑体",sans-serif;
    position:fixed;
    width: 450px;
    height: 500px;
    margin-top: -250px;
    top: 50%;
    left: 50%;
    margin-left: -225px;
    opacity: 1;
    z-index: 1000;
    border: 2px solid #0EB621 ;
    display: none;
}
.fix_wrap button:hover{
    opacity: 0.8;
}
.fix_title_wrap{
    background: #0EB621;
    height: 52px;
    text-align: center;
}
.fix_title_wrap .c10 p{
    color: #fff;
    line-height: 52px;
    font-size: 30px;
    font-weight:300;
}
.valid img{
    position: absolute;
    top: -1px;
    left: 151px;
    cursor: pointer;
}
.fix_title_wrap img{
    margin-top: 13px;
    text-align: left;
    cursor: pointer;
}
.fix_title_wrap img:hover{
    opacity: 0.8;
}
.sex .man,.sex .woman{
    display: inline-block; width: 10px;height: 10px;border: 1px solid #0EB621;border-radius: 10px;background:#0EB621;cursor: pointer;
}
.sex .woman{
    background: none;
}
.fix_form{
    height: 448px;
    background: white;
}
.fix_form .c10{
    margin-left: 37px;
    padding: 27px 0 0 0;
}
.fix_form .c10 .c3{
    text-align: center;
    font-size: 20px;
}
.fix_form .c10 .c9 select{
    width: 90%;
    height: 28px;
    outline-color: #0EB621;
}
.fix_form .c10 .c9{
    left: 20px;
}
.fix_form .c10 .c9 input{
    width: 248px;
    height: 24px ;
    outline-color: #0EB621;
}
.fix_form .c10 button{
    background:#0EB621;
    height: 40px;
    width: 357px;
    color: #fff;
    border: none;
    font-size: 26px;
    margin-left: 11px;
    border-radius: 5px;
    outline: none;
    cursor: pointer;
}
.pass{
    left: 5px;
}
.pass p{
    text-align: center;
    font-size: 12px;
}
.pass p a{
    color: #0EB621;
}
/*注册确认*/
.yy_ok{
    height: 200px;
    border-radius: 3px;
    top: 70px;
    width: 330px;
    left: 60px;
}
.ok_wrap{
    display: none;
}
/*弹窗2*/
.fix_wrap2{
    font-family:"幼圆","Comic Sans MS","黑体",sans-serif;
    position:fixed;
    width: 450px;
    height: 390px;
    margin-top: -195px;
    top: 50%;
    left: 50%;
    margin-left: -225px;
    opacity: 1;
    z-index: 1000;
    overflow: hidden;
    border: 2px solid #0EB621 ;
    display: none;
}
/*验证码*/
.get_num,.get_num2{
    display: inline-block;padding: 4px 12px 7px 12px;background: #0EB621;
    margin-left: 10px;color: #ffffff;cursor: pointer;border-radius: 3px;
    width: 82px;
    text-align: center;
}
.logo_tip{
    left: 10px;
    bottom: 17px;
    position: relative;
    font-size: 18px;
    font-weight: 500;
}
/*登录按钮*/
#log_btn{
    cursor: pointer;
    width: 250px;
    height: 35px;
    color:#FFFFFF ;
    background: #0EB621;
    outline: none;
    border: none;
    border-radius: 3px;
}
#res_btn{
    cursor: pointer;
    width: 250px;
    height: 35px;
    color:#FFFFFF ;
    background: #FFA442;
    outline: none;
    border: none;
    border-radius: 3px;
}
#res_btn:hover{
background: #A1A1A1;
}
#log_btn:hover{
background: #A1A1A1;
/*opacity: 0.6;*/
}