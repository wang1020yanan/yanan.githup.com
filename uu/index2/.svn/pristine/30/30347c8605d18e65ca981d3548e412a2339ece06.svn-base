/*回到顶部*/
.btnb{
    position: fixed;
    bottom: 90px;
    left: 100%;
    font-size: 40px;
}
.btnb i{
    color: #020202;
    border: 2px solid #000;
    padding: 4px 10px 5px;
    background: #FEFDFF;
    -webkit-border-radius:5px;
    -moz-border-radius: 5px;
    border-radius: 5px;
    -webkit-transition: background .3s ,color .3s,border .3s;
    -moz-transition:background .3s ,color .3s,border .3s;
    -ms-transition: background .3s ,color .3s,border .3s;
    -o-transition: background .3s ,color .3s,border .3s;
    transition: background .3s ,color .3s,border .3s;
}
.btnb i:hover{
    background: #8cc4cc;
    color: #ffffff;
    border: 2px solid #8cc4cc;
}
.btnb_ani{
    animation:fangun 1s;
    left: 90%;
    transform: rotate(360deg);
}
.btnb_ani2{
    animation:fangun2 1s;
    left: 100%;
    transform: rotate(360deg);
}
@keyframes fangun2
{
    from {left:90%;transform: rotate(0deg)}
    to {left:100%;transform: rotate(360deg)}
}
@keyframes fangun
{
    from {left:100%;transform: rotate(360deg)}
    to {left:90%;transform: rotate(0deg)}
}

@-moz-keyframes fangun /* Firefox */
{
    from {left:100%;}
    to {left:90%;}
}

@-webkit-keyframes fangun /* Safari and Chrome */
{
    from {left:100%;}
    to {left:90%;}
}

@-o-keyframes fangun /* Opera */
{
    from {left:100%;}
    to {left:90%;}
}
