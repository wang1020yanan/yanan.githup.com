/*抖动*/
@keyframes shut
{
    0%   {left: 0px;}
    20%  {left: -25px;}
    40%  {left: 25px;}
    60% {left: -5px;}
    80% {left: 5px;}
    100% {left: 0px;}
}

@-moz-keyframes shut /* Firefox */
{
    0%   {left: 0px;}
    20%  {left: -25px;}
    40%  {left: 25px;}
    60% {left: -5px;}
    80% {left: 5px;}
    100% {left: 0px;}
}

@-webkit-keyframes shut /* Safari 和 Chrome */
{
    0%   {left: 0px;}
    20%  {left: -25px;}
    40%  {left: 25px;}
    60% {left: -5px;}
    80% {left: 5px;}
    100% {left: 0px;}
}

@-o-keyframes shut /* Opera */
{
    0%   {left: 0px;}
    20%  {left: -25px;}
    40%  {left: 25px;}
    60% {left: -5px;}
    80% {left: 5px;}
    100% {left: 0px;}
}
.shut{
    animation:shut 0.5s;
    -moz-animation:shut 0.5s; /* Firefox */
    -webkit-animation:shut 0.5s; /* Safari and Chrome */
    -o-animation:shut 0.5s; /* Opera */
}
