body,html{
    margin: 0;
    padding: 0;
    font-family:"微软雅黑","华文细黑","Helvetica","Comic Sans MS","黑体",sans-serif;

}
body{
    display: none;
    background: #f4f4f4;
}
ul,ol{
    margin: 0;
}
*{
    margin: 0;
    padding: 0;
}
a{
    outline: none;
    text-decoration: none!important;
    border: none 0;
}
a img{
    border: 0 none;
}
li{
    list-style: none;
}

/*组件*/
.c1{
    width: 8.33333%;
}
.c2{
    width:16.666666%;
}
.c3{
    width: 25%;
}
.c4{
    width: 33.333%;
}
.c9{
    width: 75%;
}
.c7{
    width: 58.333333%;
}
.c6{
    width: 50%;
}
.c12{
    width: 100%;
}
.c2{
    width: 16.666%;
}
.c8{
    width: 66.666%;
}
.c5{
    width: 41.666%;
}
