body,html{
    margin: 0;
    padding: 0;
    font-family:"微软雅黑","华文细黑","Helvetica","Comic Sans MS","黑体",sans-serif;

}
body{
    display: none;
    background: #f4f4f4;
}
ul,ol{
    margin: 0;
}
*{
    margin: 0;
    padding: 0;
}
a{
    outline: none;
    text-decoration: none!important;
    border: none 0;
}
a img{
    border: 0 none;
}
li{
    list-style: none;
}

/*组件*/
.c1{
    width: 8.33333%;
}
.c2{
    width:16.666666%;
}
.c3{
    width: 25%;
}
.c4{
    width: 33.333%;
}
.c9{
    width: 75%;
}
.c7{
    width: 58.333333%;
}
.c6{
    width: 50%;
}
.c12{
    width: 100%;
}
.c2{
    width: 16.666%;
}
.c8{
    width: 66.666%;
}
.c5{
    width: 41.666%;
}
.c11{
    width: 0.91666%;
}
.c10{
    width: 83.33333%;
}
.c3,.c9 ,.c12 ,.c6,.c2,.c4,.c5,.c11,.c1,.c7,.c8,.c10{
    position: relative;
    float: left;
}
.container{
    padding: 0;
    width: 970px;
    margin: 0 auto;
}
.full{
    width: 100%;
    min-width: 970px;
    padding: 0;
    margin: 0;
}
.hid{
    display: none;
}




/*教员概述*/
.teacher_wrap{
    padding: 8px 0 23px 20px;
    margin-top: 20px;
    border: 1px solid #cccccc;
    box-shadow: 1px 1px 5px #cccccc;
    background: #FFFFFF;
}
.choose_tit{
    font-size: 18px;
}
.choose_tit i{
    padding-top: 15px;
    color: #2e8ded;
}
.choose_tit span{
    padding-top: 15px;
}
.tc_img{
    text-align: center;
    display: block;
}
.tc_img img{
    width: 80px;
    height: 80px;
    vertical-align:middle;
}
.tc_name{
    /*position: absolute;*/
    width: 120px;
    height: 30px;
    color: #FFFFFF;
    line-height: 30px;
    font-size: 15px;
}
.tc_name p i{
    font-size: 14px;
}
.tc_pj{
    line-height: 23px;
    padding: 15px 0 0 10px;
}
.tc_slg{
    font-size: 16px;
}
.tc_sc{
    font-size: 16px;
    /*font-weight: 600;*/
}
/*订单进度*/
.pro{
    margin-top: 30px;
}
.pro_wrap{
    padding: 20px 57px;
    border: 1px solid #cccccc;
    background: #FFFFFF;
}
.pro_tit{
    text-align: center;
    color: #0880d7;
    font-size: 18px;
}
.pro-o{
    overflow: hidden;
}
.pro-o{
    font-size: 20px;
    color: #CECBCB;
}
.pro-o i{
    font-size: 25px;
}
.pro-o span{
    width: 240px;
    display:inline-block;
    font-weight: 900;
    border-bottom: 2px solid #CECBCB;
    border-top: 2px solid #CECBCB;
    line-height: 0px;
    position: relative;
    left: -8px;
    top: -7px;
}
.pro-wrap div:nth-child(1){
    left: 32px;
}
.pro-wrap div:nth-child(3){
  right: 35px;
}
.pro-wrap div:nth-child(3) span{
}
.pro_ab {
    text-align: center;
    font-size: 16px;
    color: #CECBCB;
}
/*状态*/
.pro-wrap div:nth-child(1) i{
    color: #34a5f8;
}
.pro-wrap div:nth-child(1) span{
    border-color: #34a5f8;
}
.pro_ab div:nth-child(1){
    left: -29px;
    color: #34a5f8;
}
.pro_ab div:nth-child(2){
    left: -23px;
}
.pro_ab div:nth-child(3){
    left: -61px;
}
.pro_ab div:nth-child(4){
    left: 16px;
}
/*订单信息*/
.order{
    margin-top: 30px;
}
.order_wrap{
    padding: 20px;
    border: 1px solid #cccccc;
    background: #FFFFFF;
}
.back{
    font-size: 18px;
    margin-bottom: 35px;
}
.back p{
    color:#8C8A8A;
    font-size: 16px;
    padding-top: 3px;
}
.ul_tit{
    border-left: 6px solid #0a74f6;
    display: inline-block;
    height: 18px;
    position: relative;
    top: 3px;
}
.order_detail{
    font-size: 14px;
    padding: 20px;
}
.price{
    font-size: 25px;
    color: #F54464;
}
.pay img{
    width: 25px;
}
.pay span{
    -webkit-border-radius: 3px;
    -moz-border-radius: 3px;
    border-radius: 3px;cursor: pointer;
    font-size: 15px;
    padding: 8px;
    display: inline-block;
    border: 1px solid #CCCCCC;
    width: 125px;
}
.pay span i{
    position: absolute;
    color: #0880d7;
    margin-top: 18px;
    margin-left: -6px;
}
.pay span:nth-child(2) i{
    position: absolute;
    color: #0880d7;
    margin-top: 18px;
    margin-left: 9px;
}
.pay_btn{
    text-align: center;
    margin-bottom: 50px;
}
/*底部*/
.footer{
    height: 82px;
    background: #151818;
    color: #fafafa;
}
.foo_logo{
    line-height: 20px;
}
.foo_logo img{
    padding-top: 17px;
}
.co_name{
    padding-top: 18px;
}
.foo_wx{
    text-align: center;
}
.foo_wx  img{
    padding-top: 12px;
}
.foo_wx p{
    font-size: 18px;
    padding-top: 17px;
}
.foo_call .c1{
    text-align:right;
}
.foo_call img{
    padding-top: 17px;
    /*top: 3px;*/
    position: relative;
}
.foo_call .call{
    width: 360px;
    margin-left: 30px;
    position: relative;
    padding-top: 12px;
}
.foo_call .c11{
    margin-top: 6px;
}
.foo_call .call img{
    padding-top: 0;
}
.foo_call .add{
    top: -6px;
}
.foo_call p{
    display: inline-block;
    line-height: 4px;
}