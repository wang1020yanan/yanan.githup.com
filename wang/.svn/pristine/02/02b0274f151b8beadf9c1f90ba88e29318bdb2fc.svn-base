.mymodal {
    position: fixed;
    width: 100%;
    height: 100%;
    left: 0;
    top: 0;
    z-index: 99;
    background: rgba(0, 0, 0, 0.8);
    overflow: hidden;
}

.loading {
    position: absolute;
    top: 50%;
    border-radius: 5px;
    width: 100px;
    height: 50px;
    left: 50%;
    margin-top: -55px;
    margin-left: -50px;
    text-align: center;
    color: #FFB822;
    font-size: 4em;
}
