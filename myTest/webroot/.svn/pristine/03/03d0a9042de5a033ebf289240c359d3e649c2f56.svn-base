body, h1, h2, h3, h4, h5, h6, hr, p, blockquote, dl, dt, dd, ul, ol, li, pre, fieldset, legend, label, button, input, textarea, th, td { margin: 0; padding: 0; -webkit-tap-highlight-color: transparent; }
html { background: #f5f5f5; width: 100%; height: 100%; }
body, button, input, select, textarea {  color: #333; font: 14px/1.5 Helvetica, 'Microsoft Yahei','微软雅黑', Arial, Tahoma, "\5b8b\4f53", sans-serif; }
address, cite, dfn, em, var, i, b { font-style: normal; }
ul, ol, li, dl, dd, dt{ list-style: none; }
a { text-decoration: none; -webkit-tap-highlight-color: transparent; color: #333; }
a:hover { text-decoration: none; }
legend { color: #000; }
fieldset { border: none; }
label, button { cursor: pointer; }
h1, h2, h3, h4, h5, h6, button, input, select, textarea { font-weight: normal; }
img { border: none; display: block; -ms-interpolation-mode: bicubic; }


