body {font-size: .3rem; min-height: 100%; background-position: center; background-attachment: fixed; background-repeat: no-repeat; background-size: cover; -webkit-background-size: cover; }
.wrap { padding-bottom: 10px;position: absolute;height: 100%; zoom: 1;z-index: 9;width: 100% }

.last-page {
    content: "";
    position: fixed;
    z-index: 1;
    width: 100%;
    left: 0;
    top: 0;
    bottom: 0;
    background-image: url('../img/bg.jpg');
    -webkit-background-size: cover;
    background-size: cover;
}
.titles{margin-top: 25px;text-align: center}
.start{position: absolute;bottom: 10%}
.start img{margin: auto}


 .labels{color: #fefffa !important;background: #FFC107;  padding: 10px;  margin: 10px 0;  border-radius: 5px;}

 #J_question{padding: 10px 0;text-align: left}
/*.question-wrap{background-image: url("../img/ask1.jpg");background-size:100% 100%;height: 3rem}*/
.question{padding-top: 15px;padding-bottom: 15px;background: #fefffa;border-radius: 5px;padding-bottom: 0;margin-bottom: 10px}
#J_answers ul li{
    background: #ffffff;
