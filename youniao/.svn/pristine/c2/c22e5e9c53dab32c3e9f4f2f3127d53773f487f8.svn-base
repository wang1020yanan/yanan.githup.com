          {imgU:'images/listimg/4_c_pic.png',remaining:'28',id:'1',time:'2016-12-25',address:'上海市宝山区三门路复旦软件园高竞123号'}
        ],
        b:[{imgU:'images/listimg/5_c_pic.png',remaining:'58',id:'1',time:'2016-12-25',address:'上海市宝山区三门路复旦软件园高竞123号'},
          {imgU:'images/listimg/6_c_pic.png',remaining:'28',id:'1',time:'2016-12-25',address:'上海市宝山区三门路复旦软件园高竞123号'},
          {imgU:'images/listimg/7_c_pic.png',remaining:'13',id:'1',time:'2016-12-25',address:'上海市宝山区三门路复旦软件园高竞123号'}
        ],
        c:[{imgU:'images/listimg/9_c_pic.png',remaining:'8',id:'1',time:'2016-12-25',address:'上海市宝山区三门路复旦软件园高竞123号'},
          {imgU:'images/listimg/7_c_pic.png',remaining:'21',id:'1',time:'2016-12-25',address:'上海市宝山区三门路复旦软件园高竞123号'},
          {imgU:'images/listimg/11_c_pic.png',remaining:'23',id:'1',time:'2016-12-25',address:'上海市宝山区三门路复旦软件园高竞123号'},
          {imgU:'images/listimg/12_c_pic.png',remaining:'28',id:'1',time:'2016-12-25',address:'上海市宝山区三门路复旦软件园高竞123号'}
        ]
      };
//      初始化
      changeList('a');
//      选择器
      $(".sele-t span").click(function(){
        if($(this).parent().find('ul').hasClass('isI')){
          $(this).parent().find('ul').fadeOut(100).removeClass("isI")
        }else{
          $(this).parent().find('ul').fadeIn(100).addClass("isI")
        }
      });
      $(".sele-t li").click(function(){
        $(this).parent().fadeOut(100).removeClass('isI');
        if($(this).hasClass('grades')){
          grades=$(this).text();
          console.log(grades);
          changeList('b');
        }else{
          subject=$(this).text();
          console.log(subject);
          changeList('c');
        }
        $(this).parent().prev().html($(this).text()+'<i class="fa fa-chevron-down"></i>')
      });
      //      列表改变
      function changeList(msg){
        if(msg=='a'){
          listDetailO=listDetail.a
        }else if(msg=='b'){
          listDetailO=listDetail.b
        }else{
          listDetailO=listDetail.c
        }
        $(".actclass_list_con").empty().fadeOut(10);
        for(var i=0;i<listDetailO.length;i++){
          $(".actclass_list_con").append('<li class="actclass_li"><div class="actlis_top"><a class="actclass_li_a"><div class="actlis_top_con"> <div class="actlis_top_shengyu"> <span><img src="images/ico_lou.png">剩余'+listDetailO[i].remaining+'</span> </div> <div class="actlis_top_btn"> <div class="actlis_top_btn_t"><span><b>1500</b>元课程<b>0</b>元抢</span><button class="actlis_btn" value="'+listDetailO[i].id+'"><img src="images/ico_hand.png">立即抢</button></div> </div> </div> <img class="clis_pic_con" src="'+listDetailO[i].imgU+'"> </a> </div> <div class="actlis_bottom"> <div class="actlis_bottom_t"><a href="#" onclick="location.href="schooldetail.html""><img src="images/jg-logo.png"></a><button class="actlis_sha_btn c_btn" data-toggle="modal" data-target=".mod_sha">告诉朋友</button><button class="actlis_detail_btn c_btn" onclick="location.href="classdetail.html"">课程详情</button></div> <div class="actlis_bottom_b"><p>上课地址：上海市宝山区三门路复旦软件园高竞123号</p> </div> </div></li>')
          if(i==listDetailO.length-2){
            $(".actclass_list_con").fadeIn(300);
          }
        }
//        列表事件绑定
        $(".actlis_btn").bind("click",function(){
          window.location.href='actdetail.html?id='+$(this).val()
        });
        $(".actlis_detail_btn").bind("click",function(){
          window.location.href='classdetail.html?id='+$(this).val()
        });
      }
