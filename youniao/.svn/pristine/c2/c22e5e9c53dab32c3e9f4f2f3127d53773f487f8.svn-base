  <script>
    $(function(){
      var grades=0;
      var subject=0;
      var listDetail={
        a:[{imgU:'images/listimg/1_c_pic.png',remaining:'23',id:'1',time:'2016-12-25',address:'上海市宝山区三门路复旦软件园高竞123号'},
